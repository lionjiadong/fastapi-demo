  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI: dev",
      "type": "debugpy",
      "request": "launch",
      "module": "fastapi",
      "args": [
        "dev",
        "src/main.py" // 替换为你的应用模块路径
      ],
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "Celery: Worker",
      "type": "debugpy",
      "request": "launch",
      "module": "src.workflow.worker", // Replace 'my_module.main' with your actual module path
      "cwd": "${workspaceFolder}", // Optional: set the working directory
      "console": "integratedTerminal"
    },
    {
      "name": "Celery: Event",
      "type": "debugpy",
      "request": "launch",
      "module": "src.workflow.events", // Replace 'my_module.main' with your actual module path
      "cwd": "${workspaceFolder}", // Optional: set the working directory
      "console": "integratedTerminal"
    }
    {
      "name": "Celery: Beats",
      "type": "debugpy",
      "request": "launch",
      "module": "src.workflow.beats", // Replace 'my_module.main' with your actual module path
      "cwd": "${workspaceFolder}", // Optional: set the working directory
      "console": "integratedTerminal"
    }
  ]
}
